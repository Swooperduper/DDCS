{
  "name": "ddcs",
  "version": "3.0.1",
  "description": "backend nodeJS TypeScript development engine with full unit testing",
  "main": "src/start.ts",
  "scripts": {
    "start": "cross-env NODE_ENV=production npm install --only=prod &&  node ./dist/start.js",
    "start-dev": "nodemon --config \"./util/nodemon.json\"/",
    "test": "cross-env NODE_ENV=testing ts-node ./src/start.ts",
    "build": "rmdir /Q /S dist 2>nul && tsc --sourceMap false"
  },
  "bugs": {
    "url": "https://github.com/afinegan/DDCS/issues"
  },
  "homepage": "https://github.com/afinegan/DDCS#readme",
  "author": "Andrew Finegan <andrew.finegan@gmail.com>",
  "license": "AGPLv3",
  "dependencies": {
    "@overnightjs/core": "^1.6.9",
    "@overnightjs/logger": "^1.1.9",
    "express": "^4.17.1",
    "http-status-codes": "^1.4.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.1",
    "@types/express": "^4.17.2",
    "@types/jasmine": "^3.5.2",
    "@types/node": "^12.12.17",
    "@types/supertest": "^2.0.8",
    "cross-env": "^7.0.0",
    "jasmine": "^3.5.0",
    "nodemon": "^2.0.2",
    "supertest": "^4.0.2",
    "ts-node": "^8.5.4",
    "tsc": "^1.20150623.0",
    "tslib": "^1.10.0",
    "tslint": "^5.20.1",
    "tslint-lines-between-class-members": "^1.3.6",
    "typescript": "^3.7.3"
  }
}
